		<field type="jsnparams" subtype="jsnpanel" default="ACCESSIBILITY_SETTINGS" label="" description="" altclass="pro"/>
		<field type="jsnparams" subtype="jsnlabel" default="ONLY_IN_PRO" label="" description=""/>
