			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="enableMMTransparency" type="jsnparams" subtype="jsnradio" default="0" label="MM_TRANSPARENCY" description="MM_TRANSPARENCY_DESC" disabled="disabled">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="enableSMFX" type="jsnparams" subtype="jsnradio" default="0" label="SM_FX" description="SM_FX_DESC" disabled="disabled">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="enableSMTransparency" type="jsnparams" subtype="jsnradio" default="0" label="SM_TRANSPARENCY" description="SM_TRANSPARENCY_DESC" disabled="disabled">
			<option value="0">No</option>
			<option value="1">Yes</option>
