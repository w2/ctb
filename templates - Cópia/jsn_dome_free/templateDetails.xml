			<option value="top">TOP</option>
			<option value="middle">MIDDLE</option>
			<option value="bottom">BOTTOM</option>
		</field>
		<field name="rspAlignment" type="jsnparams" subtype="jsnlist" default="middle" label="RSP_ALIGNMENT" description="RSP_ALIGNMENT_DESC">
			<option value="top">TOP</option>
			<option value="middle">MIDDLE</option>
			<option value="bottom">BOTTOM</option>
		</field>
