		<field name="innerrightWidth" type="jsnparams" subtype="jsntext" default="23" label="INNER_RCOL_WIDTH" description="INNER_RCOL_WIDTH_DESC"/>
		<field type="jsnparams" subtype="jsnlabel" default="MISC." label="" description=""/>
		
