	<config>
	<fields name="params" addpath="/templates/jsn_dome_free/elements">
		<fieldset name="advanced">
		<field name="enableColoredLogo" type="jsnparams" subtype="jsnradio" default="0" label="ENABLE_COLORED_LOGO" description="ENABLE_COLORED_LOGO_DESC">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="logoLink" size="60" type="jsnparams" subtype="jsntext" default="" label="LOGO_LINK" description="LOGO_LINK_DESC"/>
		<field name="logoSlogan" size="60" type="jsnparams" subtype="jsntext" default="" label="LOGO_SLOGAN" description="LOGO_SLOGAN_DESC"/>
		
		<field name="templateWidth" type="jsnparams" subtype="jsnlist" default="narrow" label="TEMPLATE_WIDTH" description="TEMPLATE_WIDTH_DESC">
			<option value="narrow">NARROW</option>
			<option value="wide">WIDE</option>
			<option value="float">FLOAT</option>
		</field>
		<field name="narrowWidth" type="jsnparams" subtype="jsntext" default="960px" label="NARROW_WIDTH" description="NARROW_WIDTH_DESC"/>
		<field name="wideWidth" type="jsnparams" subtype="jsntext" default="1150px" label="WIDE_WIDTH" description="WIDE_WIDTH_DESC"/>
