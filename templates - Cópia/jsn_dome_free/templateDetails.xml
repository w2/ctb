			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="enablePrintingOptimization" type="jsnparams" subtype="jsnradio" default="0" label="PRINTING_OPTIMIZATION" description="PRINTING_OPTIMIZATION_DESC" disabled="disabled">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
		<field name="k2Style" type="jsnparams" subtype="jsnradio" default="0" label="K2_STYLE" description="K2_STYLE_DESC" disabled="disabled">
